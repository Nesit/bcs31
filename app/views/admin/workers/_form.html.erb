<%= semantic_form_for [:admin, resource] do |f| %>
    <%= f.inputs do %>
        <%= f.input :name %>
        <%= f.input :patronymic %>
        <%= f.input :job %>
        <%= f.input :phone %>
        <%= f.input :photo %>

        <%= image_tag resource.photo.thumb if resource.photo.thumb.present? %>
    <% end %>

    <%= f.actions %>
<% end %>